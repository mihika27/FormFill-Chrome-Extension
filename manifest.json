{
  "manifest_version": 3,
  "name": "Form Fill Extension",
  "version": "1.0",
  "description": "Store and retrieve form values for easy filling.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}

  
  